.page{
  width:100%;
  height: 90vh;
  position: relative;
}
.box{
   /* height: 80vh;  */
   width: 100%;
}
.box2{
  background: #999;
  opacity: 0.3;
   height: 100vh; 
}
.title{
  height: 80rpx;
  line-height: 80rpx;
  display: flex;
  font-size:30rpx;
  border-bottom: 2rpx solid #999;
}
.checkadd{
  margin-left:270rpx;
  font-size: 38rpx;
}
.share{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  border-top: 2rpx solid #eee;
  border-bottom:2rpx solid #eee;
  margin-top:10rpx;
  padding-left:30rpx;
}
.exit{
  height: 80px; 
  line-height: 80px;
  text-align: center;
  width:100%;
  background: red;
  color: #fff;
  font-size: 32rpx;
  position: fixed;
  bottom:0;
}
.model{
  width: 300px;
  height: 200px; 
  background: #eee;
  position: absolute;
  bottom:200rpx;
  z-index: 150;
  left: 60rpx; 
  border-radius: 20rpx;
  text-align: center;
  font-size: 32rpx;
  padding-top:50rpx;
}
.listOne{
  font-size:24rpx;
  color: #666;
}
.listTwo{
  border-bottom: 1rpx solid #666;
  height: 80rpx;
  line-height: 80rpx;
}
.listTwo:last-child{
  border: none;
}
.quxiao{
  width: 300px;
  height: 100rpx; 
  line-height: 100rpx; 
  background: #eee;
  position: absolute;
  bottom: 50rpx;
  z-index: 150;
  left: 60rpx; 
  border-radius: 20rpx;
  text-align: center;
  font-size: 32rpx;
